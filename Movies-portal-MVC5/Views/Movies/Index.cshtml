@model IEnumerable<Movies_portal_MVC5.Models.Movies>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-bordered table-hover">
    <thead>Movies Name</thead>
    <tbody>
        @foreach (var movie in Model)
        {
           
            <tr>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>

